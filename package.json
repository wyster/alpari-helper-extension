{
  "name": "alpari-helper-extension",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "build:dev": "npm run build -- --mode=development",
    "build:dev:watch": "npm run build:dev -- --watch",
    "lint": "eslint --ext vue,ts,js",
    "lint:fix": "npm run lint -- --fix",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "highcharts": "^7.0.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.0",
    "vue-property-decorator": "^8.0.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/chai": "^4.1.0",
    "@types/chrome": "0.0.81",
    "@types/highcharts": "^5.0.39",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.4",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-e2e-cypress": "^3.5.1",
    "@vue/cli-plugin-typescript": "^3.5.1",
    "@vue/cli-plugin-unit-mocha": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.4000",
    "vue-template-compiler": "^2.6.10",
    "webextension-polyfill-ts": "^0.9.0",
    "webpack-webextension-plugin": "^0.2.0"
  },
  "gitHooks": {
    "commit-msg": "commitlint -E GIT_PARAMS",
    "pre-commit": "lint-staged --debug"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
