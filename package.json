    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/chai": "^4.1.0",
    "@types/chrome": "0.0.81",
    "@types/highcharts": "^5.0.36",
    "@types/lodash": "^4.14.122",
    "@types/mocha": "^5.2.4",
