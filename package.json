{
  "name": "alpari-helper-extension",
  "version": "1.4.1",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "eslint --ext vue,ts",
    "build:dev": "npm run build -- --mode=development",
    "build:dev:watch": "npm run build:dev -- --watch",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "lint:fix": "npm run lint -- --fix",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "release": "standard-version"
  },
  "dependencies": {
    "file-saver": "^2.0.2",
    "highcharts": "^8.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-i18n": "^8.15.3",
    "vue-property-decorator": "^8.3.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3",
    "@commitlint/config-conventional": "^8.3",
    "@kazupon/vue-i18n-loader": "^0.5",
    "@types/chai": "^4.2.7",
    "@types/file-saver": "^2.0.1",
    "@types/highcharts": "^7",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^13",
    "@types/webpack": "^4.41.2",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "@vue/cli-plugin-babel": "^4.1",
    "@vue/cli-plugin-e2e-cypress": "^4.1",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1",
    "@vue/cli-plugin-unit-mocha": "^4.1",
    "@vue/cli-service": "^4",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-eslint": "^10.0.3",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.9",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "eslint-plugin-vue-i18n": "^0.3",
    "filemanager-webpack-plugin": "^2.0.5",
    "less": "^3.10.3",
    "less-loader": "^5.0",
    "lint-staged": "^9.5",
    "prettier": "^1.19.1",
    "standard-version": "^7.0.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.4",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-template-compiler": "^2.6.11",
    "webextension-polyfill-ts": "^0.11",
    "webpack-webextension-plugin": "^0.2.0"
  },
  "gitHooks": {
    "commit-msg": "commitlint -E GIT_PARAMS",
    "pre-commit": "lint-staged --debug"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
